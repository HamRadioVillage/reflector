# {{ ansible_managed }}
[Info]
RXFrequency=435000000
TXFrequency=435000000
Power=1
Latitude=0.0
Longitude=0.0
Height=0
Location=Nowhere
Description=Multi-Mode Repeater
URL=www.hamvillage.org

[M17 Network]
Callsign={{ m17_callsign }}
LocalPort=32010
DstName={{ m17_ref_name_and_module }}
DstAddress={{ m17_server_address }}
DstPort=17000
GainAdjustdB={{ m17_gain_adjust }}
Daemon=1
Debug={{ debug }}

[DMR Network]
Id={{ dmr_id }}
XLXFile=/opt/transcode/XLXHosts.txt
XLXReflector={{ xlx_reflector_number }}
XLXModule={{ xlx_reflector_module }}
#StartupDstId=4001
# For TG call: StartupPC=0
#StartupPC=0
#Address=127.0.0.1
Port={{ dmr_port }}
Jitter=500
# Local=62032
Password=passw0rd
# Options=
Debug={{ debug }}

[DMR Id Lookup]
File=/opt/transcode/DMRIDs.dat
Time=24

[Log]
# Logging levels, 0=No logging
DisplayLevel=1
FileLevel=1
FilePath=/var/log/M172DMR
FileRoot=M172DMR